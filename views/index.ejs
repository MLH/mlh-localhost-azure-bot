      <div class="schedule-day">Sunday, September 23</div>
      <div class="schedule-info">
        <table>
          <tr>
            <td class="schedule-entry-time">1 PM</td>
            <td class="schedule-entry-info">Closing ceremony</td>
          </tr>
          <tr>
            <td class="schedule-entry-time">3 PM</td>
            <td class="schedule-entry-info">Buses depart</td>
          </tr>
        </table>
      </div>
    </div>
  </div>
</div>
<!-- End schedule elements -->

<!-- Chat elements !-->
<div id="chat-placeholder">
  Have a question?
</div>
<div id="chat">
  <div class="logo-push-chat"></div>
  <div class="logo-wrap">
    <div class="mlh-logo"></div>
    <div class="logo-text">ACKS</div>
  </div>
  <div id="chat-area"></div>
  <input id="query-field" placeholder="Type your question here" type="text" name="query"/>
  <input id="submit-field" type="submit" value="Submit"/>
</div>
