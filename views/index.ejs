<!-- Schedule elements -->
<div class="schedule">
  <div class="schedule-title">Schedule</div>
  <div class="schedule-content">
    <div class="schedule-wrap">
      <div class="schedule-day">Friday, September 21</div>
      <div class="schedule-info">
        <table>
          <tr>
            <td class="schedule-entry-time">5 PM</td>
            <td class="schedule-entry-info">Buses arrive</td>
          </tr>
          <tr>
            <td class="schedule-entry-time"></td>
            <td class="schedule-entry-info">Check-in</td>
          </tr>
          <tr>
            <td class="schedule-entry-time">6 PM</td>
            <td class="schedule-entry-info">Walk and check-in</td>
          </tr>
          <tr>
            <td class="schedule-entry-time">7 PM</td>
            <td class="schedule-entry-info">Opening ceremony</td>
          </tr>
          <tr>
            <td class="schedule-entry-time">9 PM</td>
            <td class="schedule-entry-info">Sponsor fair</td>
          </tr>
          <tr>
            <td class="schedule-entry-time"></td>
            <td class="schedule-entry-info">Dinner</td>
          </tr>
          <tr>
            <td class="schedule-entry-time">10 PM</td>
            <td class="schedule-entry-info">Hacking begins</td>
          </tr>
        </table>
      </div>
    </div>
